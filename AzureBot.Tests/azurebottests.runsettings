<?xml version="1.0" encoding="utf-8"?>
<RunSettings>
  <TestRunParameters>
    <!-- update these with your values-->
    <add key="MicrosoftAppId" value="YOUR-MICROSOFT-APP-ID" />
    <add key="DirectLineToken" value="YOUR-DIRECTLINE-TOKEN" />
    <!-- Use the Azure.ConsoleConversation project to auth once against the bot, and use the same FromUser value here -->
    <add key="FromUser" value="YOUR-FROM-USER" />
    <!--Bot Id is the Bot Handle from your bot configuration -->
    <add key="BotId" value="YOUR-BOT-ID" />
    <Parameter name="Subscription" value="Subscription"/>
    <Parameter name="VirtualMachine" value="VirtualMachine"/>
    <Parameter name="ResourceGroup" value="ResourceGroup"/>
    <Parameter name="RunbookWithDescription" value="Runbook"/>
    <Parameter name="RunbookDescription" value="Runbook Description"/>
    <Parameter name="RunbookWithoutDescription" value="Runbook"/>
    <Parameter name="RunbookInMultipleAutomationAccounts" value="Runbook"/>
    <Parameter name="RunbookNotPublished" value="Runbook"/>
    <Parameter name="RunbookThatFails" value="Runbook"/>
    <Parameter name="RunbookWithParameters" value="Runbook"/>
    <Parameter name="RunbookOutput" value="Output"/>
    <Parameter name="AutomationAccount" value="AutomationAccount"/>
    <Parameter name="AlternativeSubscription" value="Subscription"/>
    <Parameter name="DeallocateResourcesOnCleanup" value="true"/>
  </TestRunParameters>
</RunSettings>